                android:hint="Estado do Cliente"
                android:inputType="textLongMessage"
                android:padding="0dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextCidade"
                tools:ignore="HardcodedText,PxUsage" />

            <EditText
                android:id="@+id/editTextTarifa"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_marginTop="8dp"
                android:autofillHints=""
                android:hint="Tarifa do serviço"
                android:inputType="textLongMessage"
                android:padding="0dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextEstado"
                tools:ignore="HardcodedText,PxUsage" />

            <EditText
                android:id="@+id/editTextClassePrincipal"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_marginTop="8dp"
                android:autofillHints=""
                android:hint="ClassePrincipal do (?)"
                android:inputType="textLongMessage"
                android:padding="0dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextTarifa"
                tools:ignore="HardcodedText,PxUsage" />

            <EditText
                android:id="@+id/editTextMunicipio"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_marginTop="8dp"
                android:autofillHints=""
                android:hint="Municipio do Cliente"
                android:inputType="textLongMessage"
                android:padding="0dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextClassePrincipal"
                tools:ignore="HardcodedText,PxUsage" />

            <EditText
                android:id="@+id/editTextLocaliza"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_marginTop="8dp"
                android:autofillHints=""
                android:hint="Localiza (?)"
                android:inputType="textLongMessage"
                android:padding="0dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextMunicipio"
                tools:ignore="HardcodedText,PxUsage" />

            <EditText
                android:id="@+id/editTextNIS"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_marginTop="8dp"
                android:autofillHints=""
                android:hint="NIS do Cliente"
                android:inputType="number"
                android:padding="0dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextLocaliza"
                tools:ignore="HardcodedText,PxUsage" />

            <EditText
                android:id="@+id/editTextCNPJ"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_marginTop="8dp"
                android:autofillHints=""
                android:hint="Cnpj/Cnpj da UC do Cliente"
                android:inputType="number"
                android:padding="0dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextNIS"
                tools:ignore="HardcodedText,PxUsage" />

